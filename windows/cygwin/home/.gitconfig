[alias]
	st = status
	ci = commit
	ca = commit -a -m
	lg = log --graph --all --pretty=format:'%Cred%h%Creset-%C(yellow)%d%Creset %s%Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	lgw = log --graph --since='1 weeks ago' --pretty=format:'%Cred%h%Creset-%C(yellow)%d%Creset %s%Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative 
	lgd = log --graph --since='1 days ago' --pretty=format:'%Cred%h%Creset-%C(yellow)%d%Creset %s%Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative 
	co = checkout
	br = branch
	pl = pull
	ps = push
	d = difftool
[push]
	default = simple
[core]
	fileMode = false
	quotepath = false
	editor = vim
	ignorecase = true
[color]
	ui = true
[user]
	name = 
	email = 
[diff]
	tool = vimdiff
[difftool]
	prompt = false
